    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Exporter",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "/workspaces/prometheus-shell-exporter/cmd/shell-exporter",
            "args": [
                "-f",
                "/workspaces/prometheus-shell-exporter/metrics"
            ]
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        },
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        }
    ]
}